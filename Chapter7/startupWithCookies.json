  {
    "description": "这是一个带cookies信息的post请求",
    "request": {
      "uri":"/post/with/cookies",
      "method":"post",
      "cookies": {
        "login": "true"
      },
      "json":{
        "name": "huhansan",
        "age": "18"
      }
    },
    "response":{
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      },
      "status":200,
      "json": {
        "huhansan": "success",
        "status": "1"
      }
    }
  }

